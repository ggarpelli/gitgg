{
    "name": "GuilhermeGarpelli",
    "displayName": "Gitgg - Branch File Diff",
    "description": "A Extension for comparing files across different Git branches.",
    "version": "0.0.5",
    "publisher": "ggarpelli",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.81.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:gitgg.compareFile"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "gitgg.compareFile",
                "title": "Gitgg: Compare with another branch..."
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "gitgg.compareFile",
                    "group": "navigation"
                }
            ],
            "editor/context": [
                {
                    "command": "gitgg.compareFile",
                    "group": "navigation"
                }
            ],
            "scm/resourceState/context": [
                {
                    "command": "gitgg.compareFile",
                    "group": "navigation"
                }
            ],
            "scm/resourceGroup/context": [
                {
                    "command": "gitgg.compareFile",
                    "group": "navigation"
                }
            ]
        },
        "keybindings": [
            {
                "command": "gitgg.compareFile",
                "key": "shift+\\",
                "when": "explorerResourceIs.root || editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.81.0",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "eslint": "^8.41.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^5.1.3",
        "@vscode/test-electron": "^2.3.2",
        "@types/diff": "^5.0.9",
        "@types/hogan.js": "^3.0.5"
    },
    "dependencies": {
        "simple-git": "^3.19.1",
        "diff2html": "^3.4.47",
        "diff": "^5.2.0"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/ggarpelli/gitgg.git"
    }
}