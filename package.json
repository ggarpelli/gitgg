{
    "name": "GuilhermeGarpelli",
    "displayName": "Gitgg - Branch File Diff",
    "description": "A Extension for comparing files across different Git branches.",
    "version": "0.0.7",
    "publisher": "ggarpelli",
    "icon": "icon.png",
    "repository": {
      "type": "git",
      "url": "https://github.com/ggarpelli/gitgg.git"
    },
    "engines": {
        "vscode": "^1.81.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:gitgg.compareFile"
    ],
    "main": "./dist/extension.js",
    "files": [
         "dist",
         "icon.png"
     ],
    "contributes": {
        "commands": [
            {
                "command": "gitgg.compareFile",
                "title": "Gitgg: Compare with another branch..."
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "gitgg.compareFile",
                    "group": "navigation"
                }
            ],
            "editor/context": [
                {
                    "command": "gitgg.compareFile",
                    "group": "navigation"
                }
            ],
            "scm/resourceState/context": [
                {
                    "command": "gitgg.compareFile",
                    "group": "navigation"
                }
            ],
            "scm/resourceGroup/context": [
                {
                    "command": "gitgg.compareFile",
                    "group": "navigation"
                }
            ]
        },
        "keybindings": [
            {
                "command": "gitgg.compareFile",
                "key": "shift+\\",
                "when": "explorerResourceIs.root || editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run webpack",
        "webpack": "webpack --mode production",
        "webpack-dev": "webpack --mode development --watch",
        "compile": "tsc -p ./",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/diff": "^5.0.9",
        "@types/glob": "^8.1.0",
        "@types/hogan.js": "^3.0.5",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.81.0",
        "@types/webpack-env": "^1.18.4",
        "@vscode/test-electron": "^2.3.2",
        "diff": "^5.2.0",
        "diff2html": "^3.4.47",
        "simple-git": "^3.19.1",
        "ts-loader": "^9.5.1",
        "typescript": "^5.1.3",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {}
}